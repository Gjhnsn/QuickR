    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/qrLogo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.gjhnsn.QuickR"
    },
    "web": {
      "favicon": "./assets/qrLogo.png"
    },
    "extra": {
      "eas": {
        "projectId": "202e2f24-0886-40cd-bffe-de917c33a48e"
